    <targets async="true">
      <target xsi:type="File" name="file" fileName="d:\Application\Logs\FunctionalDashboard\FD-log.txt" autoFlush="false" keepFileOpen="false" archiveAboveSize="2097152" maxArchiveFiles="100" archiveNumbering="Rolling" layout="${longdate} ${uppercase:${level}} ${message}" />
