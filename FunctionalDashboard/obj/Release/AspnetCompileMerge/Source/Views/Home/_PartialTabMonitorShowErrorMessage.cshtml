@model IEnumerable<FunctionalDashboard.ViewModels.ErrorMessage>

@{
    ViewBag.Title = "_PartialTabMonitorShowErrorMessage";
}
@{
    Layout = null;
}



<table class="table_short-wrap" style="font-size: 2em; color: black; width: 780px;">
    <caption class="caption">Message History</caption>
    <tr>
        <th class="bold fixed_column_length_500">Message</th>
        <th class="bold fixed_column_length_100">Created By</th>
        <th class="bold fixed_column_length_150">Created Date</th>
    </tr>
    @foreach (var item in Model)
    {        
        <tr>
            <td>@item.Message</td>
            <td>@item.UpdatedBy</td>
            <td>@item.UpdatedDate</td>
        </tr>      
    }
</table>

