@model List<FunctionalDashboard.Models.EventSentryStatus>

<table class="table_short">
    <caption class="caption">EventSentry Server Status</caption>
    <tr>
        <th>Server Name
        </th>
        <th>Last Update Time
        </th>
    </tr>

    @foreach (var item in Model)
    {
        if (@item.Server == "EVTAPT01")
        {
            continue;
        }
        <tr>
            <td class="bold">
                @item.Server
            </td>
            <td class="bold2">
                @item.LastUpdateTime
            </td>
        </tr>
    }

</table>
