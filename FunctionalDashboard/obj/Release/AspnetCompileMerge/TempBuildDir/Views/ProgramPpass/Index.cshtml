@model PagedList.IPagedList<FunctionalDashboard.ViewModels.PpassProgramFile>

@using PagedList.Mvc;
@using PagedList;

@{
    ViewBag.Title = "Program PPASS";
}
<script type="text/javascript">

    function tabClicked() {
        $('#tabs').click('tabsselect', function (event, ui) {

            var value = $("#tabs").tabs('option', 'active');
            
            if (sessionStorage) {
                sessionStorage.setItem("SelectedTab2", value.toString());
                
            }
        });
    }

    $(function () {
        if (sessionStorage) {
            $("#tabs").tabs({ active: sessionStorage.getItem("SelectedTab2") });
        }
        else {
            $("#tabs").tabs()
        }
    });

    //$(function () {
        
    //    $("#tabs").tabs()
       
    //});

</script>

<div class="demo">

    <div id="tabs">
        <ul>
            <li><a href="#tabs-1" onclick="tabClicked()">File</a></li>
            <li><a href="#tabs-2" onclick="tabClicked()">CUBIC Web Service</a></li>
            
        </ul>
        <div id="tabs-1">
            <div id="updateAreaFile">
                @Html.Partial("_PartialFiles", Model)
            </div>
        </div>
        <div id="tabs-2">
            <div id="updateAreaWS">
                @Html.Partial("_PartialWS", Model)
            </div>
        </div>      
    </div>
</div>




