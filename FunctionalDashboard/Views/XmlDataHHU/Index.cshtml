@model  FunctionalDashboard.Dal.DataEntity.HHUEventLog
@using System.Xml.Linq;

<link rel="stylesheet" href="~/Content/popBox.css" type="text/css" />
<script src="~/Scripts/popBox.js" type="text/javascript"></script>
<script src="~/Scripts/Common.js" type="text/javascript"></script>
<script src="~/Scripts/EventsProgramUpass.js" type="text/javascript"></script>
<script src="~/Scripts/KnowledgeBasePopup.js" type="text/javascript"></script>

@if (ViewBag.Message != null)
{
    <p style="color: red;">Error: @ViewBag.Message</p>
}

<h5>XML Data for HHU</h5>
<div id="xmldata">


    <table class="table">
        <caption class="caption">XML DATA for HHU in details</caption>
        @if (Model != null)
        {
            <tr>
                <td class="bold">ProgramID</td>
                <td>
                    @Model.ProgramID
                </td>
            </tr>
            <tr>
                <td class="bold">InstitutionID</td>
                <td>
                    @Model.InstitutionID
                </td>
            </tr>
            <tr>
                <td class="bold">Categroy</td>
                <td>
                    @Model.Category (<span class="event_id_bold">@Model.CategoryID</span>)
                </td>
            </tr>
            <tr>
                <td class="bold">Event(<span class="event_id_bold">id</span>)</td>
                <td>
                    <a href="#" id="event1"  data-id2="@Model.Category"      data-id3="@Model.CategoryID" 
                                             data-id4="@Model.Event"         data-id5="@Model.EventID" 
                                             data-id6="@User.Identity.Name"               data-id7="@DateTime.Now">
                        @Model.Event (<span class="event_id_bold">@Model.EventID</span>)
                    </a>
                </td>
            </tr>
            <tr>
                <td class="bold">Location</td>
                <td>
                    @Model.Location
                </td>
            </tr>
            <tr>
                <td class="bold">FareInstrumentID</td>
                <td>
                    @Model.FareInstrumentID
                </td>
            </tr>
            <tr>
                <td class="bold">HHUReasonCode</td>
                <td>
                    @Model.HHUReasonCode
                </td>
            </tr>
            <tr>
                <td class="bold">HHUUserID</td>
                <td>
                    @Model.HHUUserID
                </td>
            </tr>
            <tr>
                <td class="bold">CardLinkState</td>
                <td>
                    @Model.CardLinkState
                </td>
            </tr>
            <tr>
                <td class="bold">CardState</td>
                <td>
                    @Model.CardState
                </td>
            </tr>
            <tr>
                <td class="bold">CardSerialNumber</td>
                <td>
                    @Model.CardSerialNumber
                </td>
            </tr>
            <tr>
                <td class="bold">ConfiscationDatetime</td>
                <td>
                    @Model.ConfiscationDatetime
                </td>
            </tr>

            <tr>
                <td class="bold">URI</td>
                <td>
                    @Model.URI
                </td>
            </tr>
            <tr>
                <td class="bold">URIType</td>
                <td>
                    @Model.URIType
                </td>
            </tr>
            <tr>
                <td class="bold">ProcessDatetime</td>
                <td>
                    @Model.ProcessDatetime
                </td>
            </tr>
            <tr>
                <td class="bold">Process Datetime (Precision In Milliseconds)</td>
                <td>
                    @Model.ProcessDatetime.ToString("yyyy-MM-dd HH:mm:ss.fff", System.Globalization.CultureInfo.InvariantCulture)
                </td>
            </tr>
            <tr>
                <td class="bold">Log Level</td>
                <td>
                    @Model.Level
                </td>
            </tr>
            <tr>
                <td class="bold">ProcessErrorID</td>
                @if (!String.IsNullOrEmpty(Model.ProcessErrorID) && Model.ProcessErrorID != "996")
                {
                    <td class="errors">
                        @Model.ProcessErrorID
                    </td>
                }
                else
                {
                    <td class="non-error">
                        @Model.ProcessErrorID
                    </td>
                }
            </tr>
            <tr>

                <td class="bold">ProcessErrorDescr</td>
                @if (!String.IsNullOrEmpty(Model.ProcessErrorID) && Model.ProcessErrorID != "996")
                {
                    <td class="errors">
                        @Model.ProcessErrorDescr
                    </td>
                }
                else
                {
                    <td class="non-error">
                        @Model.ProcessErrorDescr
                    </td>
                }
            </tr>

                if (!String.IsNullOrEmpty(Model.StackTrace))
                {
            <tr>
                <td class="bold">StackTrace</td>
                <td class="errors">
                    @Model.StackTrace
                </td>
            </tr>
                }
        }


    </table>


</div>





