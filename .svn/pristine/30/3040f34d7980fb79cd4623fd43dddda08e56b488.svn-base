    <div class="pager" style="font-size: .5em; margin-top: -5px;">
        Page @(ViewBag.FilesPartial1.PageCount < ViewBag.FilesPartial1.PageNumber ? 0 : ViewBag.FilesPartial1.PageNumber) of @ViewBag.FilesPartial1.PageCount

