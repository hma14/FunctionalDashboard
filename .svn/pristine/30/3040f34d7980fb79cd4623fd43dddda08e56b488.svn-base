                    <td class="warnings">
                        @String.Format("{0:#,###0} ({1:#,###0})", @item.TotalErrors, @item.ClearedErrors + @item.Acknowledged)
                    </td>
                }
                else
                {
                    <td class="non-errors">
                        @item.TotalErrors
                    </td>
                }
                <td>
                    @item.IufRequests
                </td>
                <td>
                    @item.FufRequests
                </td>
                <td>
                    @item.FcfRequests
                </td>
                <td>
                    @item.IcfRequests
                </td>
            </tr>
        }
