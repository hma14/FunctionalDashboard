CREATE TABLE [dbo].[CPGFD_ErrorList] (
    [ID]              INT          IDENTITY (1, 1) NOT NULL,
    [ProgramID]       VARCHAR (10) NULL,
    [InstitutionID]   VARCHAR (10) NULL,
    [EventID]         INT          NULL,
    [CategoryID]      INT          NULL,
    [ProcessDatetime] BIGINT       NULL,
    [Status]          INT          NULL,
    [UpdatedBy]       VARCHAR (50) NULL,
    [UpdatedDatetime] DATETIME     NULL,
    CONSTRAINT [pk_ErrorList] PRIMARY KEY CLUSTERED ([ID] ASC)
);

